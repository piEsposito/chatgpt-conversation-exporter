{
    "manifest_version": 3,
    "name": "ChatGPT Conversation Exporter",
    "version": "1.0",
    "description": "Export ChatGPT conversations as JSON",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://chat.openai.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  